{
  "name": "css-frameworks-ca",
  "version": "0.2.0",
  "description": "JS2: Developing the CSS Frameworks CA further to work properly with full functionality",
  "scripts": {
    "build": "sass src/scss:src/scss",
    "start": "live-server",
    "sass": "sass --watch scss:css",
    "watch": "sass --watch src/scss:src/scss & live-server",
    "test": "npm run  --parallel test-unit test-e2e",
    "test-unit": "jest",
    "test-e2e": "cypress open",
    "test-e2e-cli": "cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/runeunhjem/js2-ca.git"
  },
  "keywords": [
    "CSS",
    "CSS",
    "Frameworks",
    "Bootstrap",
    "SCSS",
    "SASS"
  ],
  "author": "Rune Unhjem <rununh17678@stud.noroff.no>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/runeunhjem/js2-ca/issues"
  },
  "homepage": "https://github.com/runeunhjem/js2-ca#readme",
  "dependencies": {
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.10.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "cypress": "^13.4.0",
    "eslint-plugin-cypress": "^2.15.1",
    "jest": "^29.2.0",
    "jest-browser": "^0.1.0",
    "jsdom": "^22.1.0",
    "live-server": "^1.1.0",
    "sass": "^1.66.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "mjs",
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "\\.pnp\\.[^\\/]+$"
    ]
  }
}
